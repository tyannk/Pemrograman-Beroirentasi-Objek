class Mobil implements Runnable{

    String nama;

    public Mobil(String id){

        nama = id;

    }

    public void run(){

        for(int i=0;i<5; i++){

            try{

            Thread.currentThread().sleep(1000);

            }catch(InterruptedException ie){
                System.out.println("Terinterupsi");
            }

        }
    } 
    System.out.println("Thread" + nama + ": Posisi "+i);

}